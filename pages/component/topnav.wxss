.navbar{
    position: relative}.back-icon,
	.home-icon{
    width: 28px;
    height: 100%;
    position: absolute;
    transform: translateY(-50%);
    top: 50%;
    display: flex;
	}
	.back-icon{
    left: 16px;
	}
	.home-icon{
    left: 44px
	}
	.back-icon image{
    width: 28px;
    height: 28px;
    margin: auto;
	}
	.home-icon image{
    width: 20px;
    height: 20px;
    margin: auto;
	}
	.nav-title,
	.nav-icon{
    position: absolute;
